domains:
  nested:
    memory: 512
    service_provider: systemd
    nics:
      - net: lago-nested
    disks:
      - template_name: {{ nested_vm_template }}
        type: template
        name: root
        dev: vda
        format: qcow2
nets:
  lago-nested:
    gw: 192.168.210.1
    type: nat
    dhcp:
      start: 100
      end: 254
    management: true
    dns_domain_name: lago-nested.local
